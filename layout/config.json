                            "label": "script.search",
                            "range": [
                                "true",
                                "false"
                            ],
                            "required": "yes",
                            "type": "string"
                        },
                        {
                            "changeable": true,
                            "default": "true",
                            "description": "Specify whether scripts can be executed in update context.",
                            "key": "script.update",
                            "label": "script.update",
                            "range": [
